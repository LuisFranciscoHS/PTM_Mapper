<?xml version="1.0" encoding="UTF-8"?>
<actions>
        <action>
            <actionName>run</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-d64 -Xmx10g -Xmx10g -classpath %classpath no.uib.pathwaymatcher.PathwayMatcher -t ensemblList -i resources/input/ensembl -o resources/output/output_ensembl.csv -u neo4j -p neo4j2</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
        <action>
            <actionName>debug</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-Xdebug -Xrunjdwp:transport=dt_socket,server=n,address=${jpda.address} -d64 -Xmx10g -Xmx10g -classpath %classpath no.uib.pathwaymatcher.PathwayMatcher -t ensemblList -i resources/input/ensembl -o resources/output/output_ensembl.csv -u neo4j -p neo4j2</exec.args>
                <exec.executable>java</exec.executable>
                <jpda.listen>true</jpda.listen>
            </properties>
        </action>
        <action>
            <actionName>profile</actionName>
            <packagings>
                <packaging>jar</packaging>
            </packagings>
            <goals>
                <goal>process-classes</goal>
                <goal>org.codehaus.mojo:exec-maven-plugin:1.2.1:exec</goal>
            </goals>
            <properties>
                <exec.args>-d64 -Xmx10g -Xmx10g -classpath %classpath no.uib.pathwaymatcher.PathwayMatcher -t ensemblList -i resources/input/ensembl -o resources/output/output_ensembl.csv -u neo4j -p neo4j2</exec.args>
                <exec.executable>java</exec.executable>
            </properties>
        </action>
    </actions>
